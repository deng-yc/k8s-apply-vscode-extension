{
  "name": "k8s-apply-vscode-extension",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "author": "Filip Johansson",
  "publisher": "Filip Johansson",
  "repository": {
    "type": "git",
    "url": "https://github.com/filip-ph-johansson/k8s-apply-vscode-extension"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "onCommand:extension.apply",
    "onCommand:extension.delete",
    "onCommand:extension.get",
    "onCommand:extension.describe"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.apply",
        "title": "Lazy k8s: Apply"
      },
      {
        "command": "extension.delete",
        "title": "Lazy k8s: Delete"
      },
      {
        "command": "extension.get",
        "title": "Lazy k8s: Get yaml"
      },
      {
        "command": "extension.describe",
        "title": "Lazy k8s: Describe"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.apply",
          "group": "YourGroup@1",
          "when": "resourceLangId == yaml"
        },
        {
          "command": "extension.delete",
          "group": "YourGroup@1",
          "when": "resourceLangId == yaml"
        },
        {
          "command": "extension.get",
          "group": "YourGroup@1",
          "when": "resourceLangId == yaml"
        },
        {
          "command": "extension.describe",
          "group": "YourGroup@1",
          "when": "resourceLangId == yaml"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.apply",
          "group": "YourGroup@1",
          "when": "resourceLangId == yaml"
        },
        {
          "command": "extension.delete",
          "group": "YourGroup@1",
          "when": "resourceLangId == yaml"
        },
        {
          "command": "extension.get",
          "group": "YourGroup@1",
          "when": "resourceLangId == yaml"
        },
        {
          "command": "extension.describe",
          "group": "YourGroup@1",
          "when": "resourceLangId == yaml"
        }
      ]
    }
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "vscode": "^1.1.34"
  }
}
